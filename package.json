{
  "name": "resplash",
  "version": "1.0.0",
  "description": "Photo discovery application",
  "main": "index.jsx",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "format": "npm run prettier -- --write",
    "prettier": "prettier \"**/*.+(js|jsx|scss|md)\"",
    "lint": "eslint src",
    "validate": "npm run lint && npm run prettier -- --list-different",
    "serve": "webpack-dev-server --config=config/webpack.dev.js --open",
    "build:prod": "webpack -p --progress --config=config/webpack.prod.js --mode production",
    "build:dev": "webpack -p --progress --config=config/webpack.dev.js --mode development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/montecamo/resplash.git"
  },
  "author": "montecamo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/montecamo/resplash/issues"
  },
  "homepage": "https://github.com/montecamo/resplash#readme",
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/code-frame": "^7.0.0",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@svgr/webpack": "^4.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "css-loader": "^1.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.13.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.4",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.6",
    "node-sass": "^4.12.0",
    "prettier": "^1.17.0",
    "prop-types": "^15.7.2",
    "react-testing-library": "^5.9.0",
    "sass-loader": "^7.1.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.1.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
